5 задача

import math


def main(y, x, z):
    n = len(y)
    y.insert(0, 0)
    x.insert(0, 0)
    z.insert(0, 0)
    total = 0
    for i in range(1, n + 1):
        left = 42 * z[math.ceil(i / 3)] ** 2
        middle = 23 * y[n + 1 - math.ceil(i / 2)] ** 3
        right = 23 * x[n + 1 - math.ceil(i)] 
        total += 1 * (left - middle - right)
    return 1 * total
